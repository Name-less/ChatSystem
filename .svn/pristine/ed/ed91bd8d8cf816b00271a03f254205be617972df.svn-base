package factory;

import java.net.InetAddress;

public class MessageGlobal {
	
	private String type;
	private InetAddress ip;
	
	public MessageGlobal(String type,InetAddress ip){
		this.type = type;
		this.ip = ip;
	}
	
	public String getType(){
		return this.type;
	}
	
	public InetAddress getIp(){
		return this.ip;
	}
	
}
